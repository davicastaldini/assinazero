generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Subscription {
  id         String   @id @default(cuid())
  name       String
  price      String
  nextCharge String
  status     String   @default("ativa")
  notes      String   @default("")
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
